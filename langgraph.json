{
  "dependencies": [
    "langgraph>=0.2.0",
    "langgraph-sdk>=0.1.0", 
    "langchain-core>=0.3.0",
    "langchain-mcp-adapters>=0.1.0",
    "langsmith>=0.1.0",
    "aiohttp>=3.8.0",
    "requests>=2.28.0",
    "pydantic>=2.0.0"
  ],
  "graphs": {
    "mcp_auth_demo": {
      "path": "./graph.py:app",
      "description": "MCP authentication and integration best practices demo"
    }
  },
  "auth": {
    "handler": "./auth_handler.py:auth"
  },
  "env": {
    "LANGCHAIN_TRACING_V2": "true",
    "LANGCHAIN_PROJECT": "mcp-auth-demo"
  },
  "dockerfile_lines": [
    "RUN pip install aiohttp requests pydantic pytest pytest-asyncio"
  ]
}